secrets
  - secretName: traefik-app-tls-cert
openssl req -x509 -nodes -days 365 -newkey rsa:2048 -keyout tls.key -out tls.crt -subj "/CN=example.local"
kubectl create -n <ns> secret tls traefik-app-tls-cert --key=tls.key --cert=tls.crt


